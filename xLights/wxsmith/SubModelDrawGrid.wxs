<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="SubModelDrawGrid">
		<title>Select Nodes</title>
		<size>500,400</size>
		<minsize>-1,-1</minsize>
		<style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER|wxCLOSE_BOX|wxMAXIMIZE_BOX|wxMINIMIZE_BOX</style>
		<object class="wxBoxSizer" variable="wxBoxSizerMain" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>2</cols>
					<growablecols>1</growablecols>
					<growablerows>0</growablerows>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="FlexGridSizer4" member="no">
							<cols>1</cols>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON_SELECT" variable="Button_Select" member="yes">
									<label>Select</label>
									<handler function="OnButton_SelectClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON_DESELECT" variable="ButtonDeselect" member="yes">
									<label>De-Select</label>
									<handler function="OnButtonDeselectClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON_SELECT_ALL" variable="ButtonSelectAll" member="yes">
									<label>Select All</label>
									<handler function="OnButtonSelectAllClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON_SELECT_NONE" variable="ButtonSelectNone" member="yes">
									<label>Select None</label>
									<handler function="OnButtonSelectNoneClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxGrid" name="ID_GRID_NODES" variable="GridNodes" member="yes">
							<cols>1</cols>
							<rows>1</rows>
							<readonly>1</readonly>
							<labelrowheight>20</labelrowheight>
							<labelcolwidth>30</labelcolwidth>
							<defaultcolsize>30</defaultcolsize>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer1" member="no">
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_SUB_DRAW_OK" variable="ButtonSubDrawOK" member="yes">
							<label>Ok</label>
							<handler function="OnButtonSubDrawOKClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxEXPAND|wxFIXED_MINSIZE</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_SUB_DRAW_CANCEL" variable="ButtonSunDrawCancel" member="yes">
							<label>Cancel</label>
							<handler function="OnButtonSubDrawCancelClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxEXPAND|wxFIXED_MINSIZE</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxFIXED_MINSIZE</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
